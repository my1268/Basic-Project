const awe = [100, 200, 3000, 12 ,388];
function aNewFilter(item){
    return item < 3000;
}
console.log(awe.filter(aNewFilter)); //filter 예시

const as = [{text:"aaa", id: "가가"},{text:"bbb", id:"나나"}];
function asfilter(item){
    return item.text !== "aaa";
}
    console.log(as.filter(asfilter));  //filter 예시  
